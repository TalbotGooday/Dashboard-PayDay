<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="bank.payday">

	<application
		android:name="bank.payday.application.App"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">

		<activity android:name=".ui.screen.launch.LaunchActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".ui.screen.sign_in.SignInActivity" />
		<activity android:name=".ui.screen.sign_up.SignUpActivity" />
		<activity android:name=".ui.screen.transactions.TransactionsActivity" >

		</activity>
		<activity android:name=".ui.screen.dashboard.DashboardActivity" />

	</application>

</manifest>